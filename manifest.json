{
  "manifest_version": 2,
  "name": "Youtube Controller",
  "description": "Control Youtube videos from any web page",
  "author": "Maroun Baydoun",
  "version": "0.6",
  "browser_action": {
      "default_popup": "popup.html"
  },
  "background": {
    "page":"background.html",
    "persistent": false
  },
  "icons": {
    "32": "icon-small.png",
    "64": "icon-medium.png",
    "128": "icon-large.png",
    "256": "icon.png"
    },
  "permissions":["tabs", "*://*.youtube.com/*"]
}
